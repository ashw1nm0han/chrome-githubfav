{
    "manifest_version": 2, 
    
    "name": "Github Bookmarks",
    "description": "Extension to keep track of github projects that are bookmarked",
    "version": "1.0",
    
    "permissions":["bookmarks", "tabs"], 
    "browser_action": {"default_icon": "media/icon.png", "default_popup":"popup.html"},
	"content_security_policy" : "script-src 'self' ; object-src 'self'"
}